<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%# f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre %>
  <%= collection_select(:song, :genre_id, Genre.all, :id, :name) %>
  <br />

  <%= f.label :'notes_1' %>
  <%= f.text_field :'note_contents', id: "song_notes_1", name: "song[note_contents][]", value: ""%><br> 
  <%= f.label :'notes_2' %>
  <%= f.text_field :'note_contents', id: "song_notes_2", name: "song[note_contents][]", value: ""%><br> 
  
  <%= f.submit %>
<% end %>


